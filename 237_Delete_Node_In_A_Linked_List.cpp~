#include <iostream>
using namespace std;

//Definition for singly-linked list.
struct ListNode {
    int val;
    ListNode *next;
    ListNode(int x) : val(x), next(NULL) {}
};

class Solution {
public:
    void deleteNode(ListNode* node) {
      if(node==NULL){
	return;
      }
      while(node->next!=NULL){
	node->val=node->next->val;
	node=node->next;
      }
      node=NULL;
    }
};
