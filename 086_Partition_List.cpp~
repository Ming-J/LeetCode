#include <iostream>
using namespace std;
// Definition for singly-linked list.
struct ListNode {
  int val;
  ListNode *next;
  ListNode(int x) : val(x), next(NULL) {}
};

class Solution {
public:
  ListNode* partition(ListNode* head, int x) {
    ListNode* cur = head;
    ListNode* greaterThanX = NULL;
    ListNode* greaterThanXTail = NULL;
    while(head != NULL){
      if(cur->val > x){
	if(!greaterThanXTail&&!greaterThanX){
	  greaterThanX = greaterThanXTail = cur;
	}else{
	  greaterThanXTail -> next = 
	}
      }
    }
  }
};
