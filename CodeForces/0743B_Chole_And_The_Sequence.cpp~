#include <iostream>
#include <vector>
using namespace std;

class Sequence{
public:
  Sequence(size_t n){
    // 1
    seq.assign(1,1);
    //n - 1
    for(size_t i = 2; i <= n; ++i){
      seq.insert(seq.end(),i);
      seq.insert(seq.end(), seq.rbegin() + 1, seq.rend());
    }
  }

  int operator[](const size_t i){
    return seq[i-1];
  }

private:
  vector<int> seq;
};


int main(){
  size_t n = 30, t = 671088640;
  //cin>>n>>t;
  Sequence te(n);
  cout<<te[t]<<endl;
}
