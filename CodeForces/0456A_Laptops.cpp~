#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;

int main(){
  int n;
  cin>>n;
  vector<pair<int, int> > laptops;
  for(int i = 0; i < n; ++i){
    int a, b;
    cin>>a>>b;
    laptops.push_back({a,b});
  }
  sort(laptops.begin(),laptops.end());
  for(size_t i = 0; i < laptops.size(); ++i){
    int j = i + 1;
    int curPrice = laptops[i].second;
    while(j < laptops.size()){
      if(curPrice > laptops[j].second){
	cout<<"Happy Alex";
	return 0;;
      }
      ++j;
    }
  }
  cout<<"Poor Alex"<<endl;
  return 0;
}
