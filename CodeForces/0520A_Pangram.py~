import sys

def main():
    n = int(sys.readline().strip())
    if n < 52:
        print("NO")
    else:
        lst = []*56
        for c in sys.readline().strip():
            lst[ord(c)] += 1
        for x in lst:
            if x == 0:
                print("NO")
                return
        print("YES")

if __name__ == ("__main__"):
    main()
